Page{background: #f4f4f4;}
.header{
	display: flex;
	padding:140rpx 50rpx 60rpx 50rpx;
	background-image: linear-gradient(120deg, #fccb90 0%, #d57eeb 100%);
	justify-content: center;
}
.header image{
	width: 150rpx;
	height: 150rpx;
	border-radius: 100rpx;
}
.icon-back{
	position: absolute;
	left:40rpx;
	top:60rpx;
	color: #fff;
	font-weight: 100;
	font-size: 36rpx;
}
.intro{padding:50rpx;font-size: 40rpx;text-indent:80rpx;color: #555}
.help{font-size: 32rpx;padding:30rpx 50rpx;line-height: 60rpx;}
.q{padding: 0 50rpx;font-size: 30rpx;color:#666;}

.buttons{margin:30rpx 30rpx;background: #fff;padding:30rpx;border-radius: 20rpx;}
.buttons button{font-size:34rpx;margin-bottom:30rpx;}
.buttons h2{font-size:30rpx;text-align: center;display: block;margin-bottom:20rpx;}