  height:110rpx;
  background:#c00;
  color: #fff;
  font-size: 103rpx;
  z-index: 999;
  border-radius: 20rpx;
  box-shadow: 0 2px 2px rgba(0, 0, 0, 0.5);
}
.eliteTitle{
	font-size: 36rpx;
	padding-top:40rpx;
	padding-bottom:20rpx;
	display: flex;
	margin:0 40rpx;
	justify-content: space-between;
}
.member_rank{
	color: #c00;
	font-size: 36rpx;
}

.eliteH2{
	text-align: center;
	display: block;
	padding-top:20rpx;
	font-size:36rpx;
}
.wangming-list{
    display: flex;
    flex-flow: wrap;
    margin:30rpx 0 0rpx 35rpx;
}
.wangming-item {
    width: 46%;
    background: #fffff0;
    border-radius: 20rpx;
		line-height: 60rpx;
    color:#333;
    text-align: center;
    margin-right: 25rpx;
    margin-bottom: 40rpx;
		box-shadow: 0 2px 2px rgba(0, 0, 0, 0.1);
}
.wangming-item .top{
	padding:30rpx;
	height:110rpx;
}
.wangming-item .top .t{
	max-width: 360rpx;
	font-size: 36rpx;
	overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.wangming-item .bottom{
	background:#fff;
	border-top:1rpx solid #eee;
	border-bottom-left-radius: 20rpx;
	border-bottom-right-radius: 20rpx;
	color: #666;
	font-weight: 300;
	padding:10rpx;
	display: flex;
	justify-content: space-around;
}
.wangming-item .bottom .username{
	max-width: 150rpx;
	text-overflow: ellipsis;
  white-space: nowrap;
	overflow: hidden;
}
.wangming-item .bottom .onclick{
	font-size: 24rpx;
}
.wangming-item .bottom .onclick span{
	font-size: 30rpx;
	color: #ed6ea0;
}
.wangming-item .bottom .ed{
	color: #ed6ea0
}

