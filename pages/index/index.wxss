Page{background: #eee;}

.swiper-tab{  
    width: 100%;   
    text-align: center;  
    line-height: 80rpx;
		margin-top:20rpx;
}  
.swiper-tab-list{  font-size: 36rpx;  
    display: inline-block;  
    width: 50%;  
		font-weight: 300;
    color: #272727;  
}  
.on{ color: #c00;}  

.indexMoreButton{
	margin:0rpx 40rpx 30rpx;
}

.swiper-box{ display: block; height: 100%; width: 100%; overflow: hidden; }  
.swiper-box view{  
    text-align: center;  
}  

.j_header {
  position: fixed;
  width: 100%;
  top: 0;
  background-image: linear-gradient(120deg, #f093fb 0%, #f5576c 100%);
  color: #fff;
  z-index: 999;
	display: flex;
}
.j-title {
	position: absolute;
  text-align: center;
  max-width: 360rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  font-size: 36rpx;
  color: #fff;
  font-weight: 300;
	z-index: 999;
}
.j-left{
	position: absolute;
	left: 0rpx;
  right: 0;
  bottom: 0;
	display: flex;
}
button[plain]{ border:0;padding:0;margin:0;margin-top:30rpx;margin-left:50rpx;color: #fff; }
.search{
	position: absolute;
	left: 100rpx;
  right: 0;
  bottom: 0;
  margin: auto;
	display: flex;
	background:#f8f8f8;
	margin-left:50rpx;
	height: 46rpx;
	line-height: 46rpx;
	border-radius: 15rpx;
	padding:10rpx 30rpx;
	color: #999;
	width: 40%;
}
.search .icon{margin-right:10rpx;}

.covers {
  position: absolute;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.65);
  width: 100%;
  height: 100%;
  z-index: 999;
}

.addGif {
  position: fixed;
  top: 140rpx;
  right: 50rpx;
  width: 300px;
  height: 390px;
  z-index: 9999;
}

.h2{margin:20rpx 20rpx 10rpx;text-align: center;font-size:36rpx;color: #666;display: block;}
.baoxiaoListViews{margin:30rpx;background:#fff;padding:40rpx;border-radius: 20rpx;text-align: left;}
.baoxiaoListViews h3{font-size: 36rpx;display:block;padding-bottom:20rpx;font-weight: 300;line-height: 60rpx;}
.baoxiaoListViews p{font-size:30rpx;color: #666;}

swiper{height:300rpx;font-weight: 300}
.swiper_item{text-align:center;font-size:50rpx;line-height:140rpx;height:300rpx;}
swiper .img{height:300rpx;width:100%}

.ads{margin-bottom:20rpx;}

.layout_vertical{
  flex-flow: row wrap;
  display: flex;
  flex-direction: row;
  margin:10rpx 20rpx 20rpx;
}
.listViews{flex:1;margin:10rpx 8rpx;background:#fff;padding:10rpx;border-radius: 10rpx;text-align: center}


.add{
  position: fixed;
  bottom:100rpx;
  right:100rpx;
